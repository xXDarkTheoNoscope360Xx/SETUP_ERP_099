  <Sequence sap2010:Annotation.AnnotationText="--------------------------------------------------------------------- DO NOT TOUCH --------------------------------------------------------------------&#xA;This workflow file is used to stop the execution of the robot and ask a validation of the business expert of the process.&#xA;&#xA;In_Arguments :&#xA;  - In_BreakPoint : This argument trigger the message box or not. If your are in a testing phase, put it to True to display the message box. If you are in a production environment, put it to False.&#xA;  - In_DataToShowLabel : This is the label or the name of the data you want to diaplay to the user&#xA;  - In_DataToShowContent : This is the content of the data you want to display to the user" DisplayName="BreakPoint" sap2010:WorkflowViewState.IdRef="Sequence_1" sads:DebugSymbol.Symbol="">
    <If Condition="[In_BreakPoint]" DisplayName="If BreakPoint" sap2010:WorkflowViewState.IdRef="If_1">
      <If.Then>
        <Sequence DisplayName="Display Message Box" sap2010:WorkflowViewState.IdRef="Sequence_2">
          <Sequence.Variables>
            <Variable x:TypeArguments="x:String" Name="buttonChoose" />
          </Sequence.Variables>
          <If Condition="[String.IsNullOrEmpty(In_DataToShowContent)]" sap2010:WorkflowViewState.IdRef="If_2">
            <If.Then>
              <ui:MessageBox AutoCloseAfter="00:00:00" Buttons="YesNo" Caption="Point d'arrêt" ChosenButton="[buttonChoose]" DisplayName="Message box" sap2010:WorkflowViewState.IdRef="MessageBox_1" Text="[&quot;Did you validate those information ?&quot;]" TopMost="True" />
            </If.Then>
            <If.Else>
              <ui:MessageBox AutoCloseAfter="00:00:00" Buttons="YesNo" Caption="Point d'arrêt" ChosenButton="[buttonChoose]" DisplayName="Message box" sap2010:WorkflowViewState.IdRef="MessageBox_2" Text="[In_DataToShowLabel + &quot; : &quot; + In_DataToShowContent]" TopMost="True" />
            </If.Else>
          </If>
          <If Condition="[buttonChoose = &quot;No&quot;]" sap2010:WorkflowViewState.IdRef="If_3">
            <If.Then>
              <TerminateWorkflow DisplayName="Fin du processus" sap2010:WorkflowViewState.IdRef="TerminateWorkflow_1" Reason="Process stopped by user" />
            </If.Then>
          </If>
        </Sequence>
      </If.Then>
    </If>
  </Sequence>
